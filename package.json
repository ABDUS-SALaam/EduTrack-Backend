{
  "name": "edutrack",
  "version": "1.0.0",
  "description": "Node application for eduTrack",
  "main": "dist/index.ts",
  "scripts": {
    "build": "tsc",
    "start:dev": "npm run build && cross-env NODE_ENV=development node ./dist/index.js",
    "start:prod": "npm run build && cross-env NODE_ENV=production node ./dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node ./src/index.ts"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "globals": "^15.14.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.5.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mongoose": "^8.9.3",
    "multer": "^1.4.5-lts.1"
  }
}
